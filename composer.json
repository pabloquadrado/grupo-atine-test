{
  "autoload": {
      "psr-4": {
          "App\\": "src/"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "App\\Tests\\": "tests/"
      }
  },
  "require-dev": {
      "phpunit/phpunit": "^9.5"
  },
  "scripts": {
      "up": "docker-compose up -d",
      "stop": "docker-compose stop",
      "test": "docker-compose run php-fpm ./vendor/bin/phpunit"
  }
}
